{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "MusicOne",
  "version": "1.2.1",
  "identifier": "com.musicone.sync",
  "build": {
    "beforeDevCommand": "vite",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "tsc && vite build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "MusicOne CloudSync",
        "width": 800,
        "height": 600,
        "resizable": false,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDcwMUY3NzQ4NjI4NEQ5M0YKUldRLzJZUmlTSGNmY0hic09xSTdmQU1IQWkvWVdtTG5OMXdQK0x5R1VSQjVPdEowT1l5UTVpU3kK",
      "endpoints": [
        "https://github.com/edi/M1-Sync/releases/latest/download/latest.json"
      ]
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": [
      "app",
      "msi"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
